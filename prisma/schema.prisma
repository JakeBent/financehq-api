generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgres"
  url      = env("POSTGRES_URL")
}

model User {
  id       Int @id @default(autoincrement())
  email    String @unique
  password String
  name     String?
}